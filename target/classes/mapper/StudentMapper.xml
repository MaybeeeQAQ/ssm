<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hand.ssm.mapper.StudentMapper">
   <select id="getAllStudent" resultType="com.hand.ssm.pojo.Student">
        select * from student
   </select>

   <insert id="InsertStudent" parameterType="com.hand.ssm.pojo.Student">
        insert into student value (#{sno},#{sname},#{score});
   </insert>
</mapper>